<Window x:Class="L3S06"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="L3S06" Height="300" Width="300">
    <RichTextBox>
        <FlowDocument>
            <Paragraph>Some text paragraph with ellipse
                <InlineUIContainer>
                    <Ellipse Width="10" Height="10" Fill="Red"/>
                </InlineUIContainer>
            </Paragraph>
            <List>
                <ListItem>
                    <Paragraph>
                        Item 1
                    </Paragraph>
                </ListItem>
                <ListItem>
                    <Paragraph>
                        Item 2
                    </Paragraph>
                </ListItem>
            </List>
            <BlockUIContainer>
                <StackPanel>
                    <Button>Button</Button>
                    <CheckBox>CheckBox</CheckBox>
                </StackPanel>
            </BlockUIContainer>
        </FlowDocument>
    </RichTextBox>
</Window>
